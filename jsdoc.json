{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs)"
    },
    "plugins": [],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "verbose": true
    }
  }
  